/**
 * rofi -dump-theme output.
 * Rofi version: 1.7.8+wayland1
 **/

* {
    font-family: "JetBrainsMono Nerd Font";
    background-color: #00000300; 
}

prompt{
    content: "select wifi";
    padding:0px;
    margin:0px;
    font-size: 8px;
}

configuration {
    show-icons: true;
}

window
{
    padding:0px;
    background-color: #000003CC;
    border: 4px;
    border-radius: 10px;
    width:45%;
    height:40%;
    transparency: "real";
}

listview
{
    spacing:5px;
    orientation: vertical;
}

element
{
    padding:5px;
    background-color: transparent;
    spacing:10px;
}

element selected
{
    background-color: #838A7A;
    horizontal-align: 0;
}

element-icon selected
{
    background-color: transparent;
    horizontal-align: 0;
}

element-text selected{
    text-color : #000003;
}
mainbox {
    padding: 10px;
    border: 4px;
    border-radius: 10px;
}

inputbar {
    padding:    1px ;
    spacing:    0px ;
    text-color: #DB5F65;
    children:   [ "prompt","textbox-prompt-colon","entry"];
}

case-indicator {
    spacing:    0;
    text-color: #DB5F65;
}
entry {
    text-color:        #DB5F65;
    cursor:            text;
    spacing:           0;
    placeholder-color: Gray;
    placeholder:       "Type to filter";
}
prompt {
    spacing:    0;
    text-color: #DB5F65;
}
textbox-prompt-colon {
    margin:     0px 0.3000em 0.0000em 0.0000em ;
    expand:     false;
    str:        ":";
    text-color: inherit;
}

scrollbar {
    width:        4px ;
    padding:      0;
    handle-width: 8px ;
    border:       0;
    handle-color: #D0ECE0;
}

element-text
{
    text-color: #DB5F65;
    background-color: transparent;
}
element-icon
{
    size:20px;
}